export PATH=$PATH:$HOME/myconf/bin
export HISTTIMEFORMAT="%d/%m/%y %T "

export ALTERNATE_EDITOR=emacs EDITOR=emacsclient VISUAL=emacsclient
# In WSL if using native Emacs for Windows
# export ALTERNATE_EDITOR=emacs.exe EDITOR=emacsclientw.exe VISUAL=emacsclientw.exe

export NO_AT_BRIDGE=1

# enable XON/XOFF flow control
# Then CTRL+S will give you a (i-search) (non reverse).
stty -ixon